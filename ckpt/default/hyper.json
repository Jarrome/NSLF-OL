      0,
      1,
      2,
      3,
      4,
      5
    ],
    "latent_in": [
      3
    ],
    "weight_norm": true
  },
  "encoder_name": "di_encoder",
  "encoder_specs": {
    "per_point_feat": [
      6,
      32,
      64,
      256
    ],
    "bn": {
      "class": "BatchNorm"
